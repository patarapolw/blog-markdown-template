{
  "name": "jpdiary",
  "version": "1.0.0",
  "description": "polv&#39;s Japanese learning diary",
  "author": "Pacharapol Withayasakpunt",
  "private": true,
  "scripts": {
    "dev": "run-p 'dev:**'",
    "dev:nuxt": "cross-env BUILD=1 nuxt",
    "dev:lambda": "wait-on build/idx.json build/raw.json && netlify-lambda serve lambda",
    "build": "run-p 'build:**'",
    "build:nuxt": "cross-env BUILD=1 nuxt build && nuxt export",
    "build:lambda": "wait-on build/idx.json build/raw.json && netlify-lambda build lambda",
    "postinstall": "netlify-lambda install",
    "start": "nuxt serve",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "lunr": "^2.3.8",
